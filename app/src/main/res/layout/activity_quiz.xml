        <RadioButton
            android:id="@+id/radioButton23"
            android:layout_width="125dp"
            android:layout_height="wrap_content"
            android:text="om 08:00" />

        <RadioButton
            android:id="@+id/radioButton22"
            android:layout_width="125dp"
            android:layout_height="wrap_content"
            android:text="om 7:30" />
